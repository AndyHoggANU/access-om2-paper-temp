@article{Barnier2006,
	Abstract = {Series of sensitivity tests were performed with az-coordinate, global eddy-permitting (1/4?) ocean/sea-ice model (the ORCA-R025 model configuration developed for the DRAKKAR project) to carefully evaluate the impact of recent state-of-the-art numerical schemes on model solutions. The combination of an energy?enstrophy conserving (EEN) scheme for momentum advection with a partial step (PS) representation of the bottom topography yields significant improvements in the mean circulation. Well known biases in the representation of western boundary currents, such as in the Atlantic the detachment of the Gulf Stream, the path of the North Atlantic Current, the location of the Confluence, and the strength of the Zapiola Eddy in the south Atlantic, are partly corrected. Similar improvements are found in the Pacific, Indian, and Southern Oceans, and characteristics of the mean flow are generally much closer to observations. Comparisons with other state-of-the-art models show that the ORCA-R025 configuration generally performs better at similar resolution. In addition, the model solution is often comparable to solutions obtained at 1/6 or 1/10? resolution in some aspects concerning mean flow patterns and distribution of eddy kinetic energy. Although the reasons for these improvements are not analyzed in detail in this paper, evidence is shown that the combination of EEN with PS reduces numerical noise near the bottom, which is likely to affect current?topography interactions in a systematic way. We conclude that significant corrections of the mean biases presently seen in general circulation model solutions at eddy-permitting resolution can still be expected from the development of numerical methods, which represent an alternative to increasing resolution.},
	Author = {Barnier, Bernard and Madec, Gurvan and Penduff, Thierry and Molines, Jean Marc and Treguier, Anne Marie and {Le Sommer}, Julien and Beckmann, Aike and Biastoch, Arne and B{\"{o}}ning, Claus and Dengg, Joachim and Derval, Corine and Durand, Edm{\'{e}}e and Gulev, Sergei and Remy, Elizabeth and Talandier, Claude and Theetten, S{\'{e}}bastien and Maltrud, Mathew and McClean, Julie and {De Cuevas}, Beverly},
	Doi = {10.1007/s10236-006-0082-1},
	Isbn = {1616-7341},
	Issn = {16167341},
	Journal = {Ocean Dynamics},
	Keywords = {Eddy-permitting ocean model,Eddy/topography interactions,Global ocean,Momentum advection scheme,Partial step topography},
	Number = {5-6},
	Pages = {543--567},
	Title = {{Impact of partial steps and momentum advection schemes in a global ocean circulation model at eddy-permitting resolution}},
	Volume = {56},
	Year = {2006},
	Bdsk-Url-1 = {https://doi.org/10.1007/s10236-006-0082-1}}

